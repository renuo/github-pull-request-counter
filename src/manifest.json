{
  "manifest_version": 3,
  "author": "Renuo AG",
  "name": "Github Pull Request Counter",
  "description": "A chrome extension to remove mental load when working with pull requests.",
  "version": "0.0.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "32": "icon.png"
    }
  },
  "options_page": "options.html",
  "permissions": [
    "alarms"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
