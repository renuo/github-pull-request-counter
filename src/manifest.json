{
  "manifest_version": 2,
  "author": "Renuo AG",
  "name": "Github Pull Request Counter",
  "description": "A chrome extension to remove mental load when working with pull requests.",
  "version": "1.0.7",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "alarms",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
